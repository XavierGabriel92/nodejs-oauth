{
  "name": "bot-crypto",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node build/index.js",
    "dev": "NODE_ENV=development nodemon --watch 'src/' --exec 'npm run build:dev' -e ts",
    "build": "rm -rf ./build && tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "build:dev": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json && node build/index.js",
    "test": "npm run test:unit:coverage",
    "test:unit": "jest -c ./jest.config.ts",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/config": "^0.0.41",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "husk": "^0.5.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "ts-alias": "^0.0.3-1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "tsc-alias": "^1.6.5",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "config": "^3.3.7",
    "pino": "^7.9.2",
    "pino-pretty": "^7.6.0"
  }
}
